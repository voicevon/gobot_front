    {
        "id": "8408438a8d4451d9",
        "type": "function",
        "z": "25c5022b18113ab0",
        "name": "old verion ui",
        "func": "\nvar saved = flow.get('channels_state_for_old');\n// node.warn(saved[0]);\nvar payload = msg.payload;\n\n// var rx_buffer = new ArrayBuffer(payload.length);\n\n\nfor (var i = 0; i < payload.length; i++) {\n    // var rx_code = payload.charCodeAt(i);\n    // var saved_code = saved[i];\n    // var x = [rx_code, saved_code]\n    // node.warn(x);\n    \n    if (payload.charAt(i) != saved.charAt(i)) {\n        // saved[i] = rx_code;\n        saved = msg.payload;\n        flow.set('channels_state_for_old', saved);\n        var tx_topic = 'actp/001/u';\n        var tx_paylod = 'OFF';\n        var node_id = 0;\n        var channel_id = 0;\n        node_id = Math.trunc(i/14);   // be interger\n        channel_id = i % 14;\n        // var x = [i, node_id, channel_id];\n        // node.warn(x);\n\n        tx_topic += (node_id + 'p' + channel_id);\n        if (msg.payload.charAt(i)=='T')\n            tx_paylod = 'ON';\n        msg.topic = tx_topic;\n        msg.payload = tx_paylod;\n        return msg;\n    }\n}\nmsg = null;\nreturn msg;\n",
        "outputs": 1,
        "noerr": 0,
        "initialize": "// Code added here will be run once\n// whenever the node is started.\n\nvar saved = '';\n\nflow.set('channels_state_for_old', saved);",
        "finalize": "",
